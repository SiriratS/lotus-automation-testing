{
  "name": "lotuss-playwright-automation",
  "version": "1.0.0",
  "description": "Playwright automation testing for Lotus's e-commerce website",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:codegen": "playwright codegen https://www.lotuss.com/th",
    "test:demo": "playwright test --config=playwright.demo.config.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.48.0",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "eslint-plugin-playwright": "^2.4.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {}
}